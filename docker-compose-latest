version: '3'
services:
  mongo:
    image: mongo
    volumes:
      - newvol:/data/tns/
    networks:
      - newnet
  mongo_express:
    image: mongo-express
    volumes:
      - newvol:/data/tns/
    networks:
      - newnet
volumes:
  newvol:
    external: true
networks:
  newnet:
    external: true
